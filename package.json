  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-table": "^6.10.3"
  },
  "resolutions": {
    "@uifabric/icons": "7.3.5"
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "postversion": "git push",
    "build": "rm -rf lib/ && npx babel src/ -d lib/ --copy-files && rm -rf lib/stories",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build-icons": "npx @svgr/cli --template src/utils/svgr.js -d src/components/Svg src/assets/svg"
  },
  "standard": {
    "ignore": [
      "node_modules/**",
      "__tests__/**"
    ],
    "env": [
      "jest",
      "node"
    ],
    "parser": "babel-eslint"
  },
  "svgr": {
    "prettierConfig": {
      "singleQuote": true,
      "semi": false
    },
    "svgProps": {
      "width": 24,
      "height": 24,
      "stroke": "white",
      "fill": "white"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.5.5",
