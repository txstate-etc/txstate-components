{
  "name": "@txstate-mws/txstate-react",
  "version": "1.1.5",
  "homepage": "https://txstate-etc.github.io/txstate-components/",
  "repository": {
    "url": "https://github.com/txstate-etc/txstate-components"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@date-io/dayjs": "^1.3.9",
    "@material-ui/core": "^4.5.2",
    "@material-ui/pickers": "^3.2.2",
    "dayjs": "^1.8.15",
    "draft-js": "^0.11.0",
    "draftjs-to-html": "^0.8.4",
    "fast-deep-equal": "^2.0.1",
    "lodash": "^4.17.15",
    "office-ui-fabric-react": "^7.60.0",
    "prop-types": "^15.7.2",
    "react-data-table-component": "^3.6.1",
    "react-draft-wysiwyg": "^1.13.2",
    "react-email-editor": "^0.9.9",
    "react-table": "6.10.3",
    "rxjs": "^6.5.2",
    "shortid": "^2.2.14",
    "styled-components": "4.3.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-table": "^6.10.3"
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "postversion": "git push",
    "build": "rm -rf lib/ && npx babel src/ -d lib/ --copy-files && rm -rf lib/stories",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build-icons": "npx @svgr/cli --template src/utils/svgr.js -d src/components/Svg src/assets/svg"
  },
  "standard": {
    "ignore": [
      "node_modules/**",
      "__tests__/**"
    ],
    "env": [
      "jest",
      "node"
    ],
    "parser": "babel-eslint"
  },
  "svgr": {
    "prettierConfig": {
      "singleQuote": true,
      "semi": false
    },
    "svgProps": {
      "width": 24,
      "height": 24,
      "stroke": "white",
      "fill": "white"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addon-storysource": "^5.3.9",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@svgr/cli": "^4.3.3",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "2.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.19.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "color": "^3.1.2",
    "eslint": "^6.2.1",
    "eslint-config-standard-with-typescript": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "standard": "^13.1.0",
    "typescript": "^3.6.4"
  }
}
