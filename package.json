    "url": "https://github.com/txstate-etc/txstate-components"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@date-io/dayjs": "2.4.0",
    "@material-ui/core": "4.9.2",
    "@material-ui/pickers": "3.2.10",
    "dayjs": "1.8.20",
    "draft-js": "0.11.4",
